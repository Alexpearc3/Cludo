+import pygame
+
+
+class Room:
+    def room():
+        # initializing the constructor
+        pygame.init()
+        # title of window
+        pygame.display.set_caption('Room')
+        # icon of window
+        icon = pygame.image.load(namehint='Clue logo', filename='clue_logo.png')
+        pygame.display.set_icon(icon)
+        # size of window
+        screen = pygame.display.set_mode((1400, 800))
+        done = False
+        while not done:
+            for event in pygame.event.get():
+                # quit window of room
+                if event.type == pygame.QUIT:
+                    done = True
+        pygame.display.flip()
+
+    room()
